{
  "name": "mmss.pro",
  "base_url": "https://chisha.tssuy.cn/fby.php",
  "steps": [
    {
      "action": "request_js",
      "url": "{{base_url}}",
      "priority_scraper": true,
      "max_retries": 3,
      "timeout": 15
    },
    {
      "action": "parse_json",
      "save_as": "domain_list"
    }
  ],
  "result": {
    "domains": "{{domain_list}}",
    "wd_url": "/index.php/vod/search.html?wd=<*>?ref=HEIMAO"
  }
}
